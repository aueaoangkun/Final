<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="th.ac.kku.cis.mobileapp.finalchat">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Finalchat">
        <activity
            android:name="th.ac.kku.cis.mobileapp.finalchat.ChatActivity"
            android:exported="false"/>
        <activity android:name="th.ac.kku.cis.mobileapp.finalchat.SignUp" />
        <activity
            android:name="th.ac.kku.cis.mobileapp.finalchat.LogIn"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="th.ac.kku.cis.mobileapp.finalchat.MainActivity"></activity>
    </application>

</manifest>